{
  "routes": [
    { "route": "/api/*", "statusCode": 404 }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "cache-control": "public, max-age=600"
  }
}
